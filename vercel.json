{
  "buildCommand": "cd my-app && npm install && npm run build",
  "outputDirectory": "my-app/.next",
  "installCommand": "cd my-app && npm install",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
