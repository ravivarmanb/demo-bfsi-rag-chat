{
  "version": 2,
  "builds": [
    {
      "src": "api/minimal.py",
      "use": "@vercel/python"
    },
    {
      "src": "my-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/minimal.py"
    },
    {
      "src": "/(.*)",
      "dest": "/my-app/$1"
    }
  ]
}
